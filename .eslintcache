data => {\r\n            dispatch(SetUserProfile(data));\r\n        })\r\n    }\r\n}\r\n\r\nexport const getUserStatus = (userID) => {\r\n    return (dispatch) => {\r\n        profileAPI.getUserStatus(userID).then(response => {\r\n            dispatch(SetUserStatus(response.data));\r\n            if (response.data === null || response.data === '') {\r\n                dispatch(SetUserStatus('...'));\r\n            }\r\n        })\r\n    }\r\n}\r\n\r\nexport const updateUserStatus = (status) => {\r\n    return (dispatch) => {\r\n        profileAPI.updateUserStatus(status).then(response => {\r\n            if (response.data.resultCode === 0) {\r\n                dispatch(SetUserStatus(status));\r\n            }\r\n        })\r\n    }\r\n}\r\n// thunks thunks thunks\r\n\r\nexport default profileReduser;","D:\\Programming\\React\\projects\\01_firts_project\\portfolio_project\\src\\Redux\\Reducer-Messages.js",[],"D:\\Programming\\React\\projects\\01_firts_project\\portfolio_project\\src\\Redux\\store.js",[],"D:\\Programming\\React\\projects\\01_firts_project\\portfolio_project\\src\\components\\MainPageComponents\\Profile\\MainContentBlock\\CreateNewPost\\CreateNewPostContainer.js",["219"],"import React from 'react';\r\nimport {addPost, changeTextarea} from \"../../../../../Redux/Reducer-Profile\";\r\nimport CreateNewPost from \"./CreateNewPost\";\r\nimport {connect} from \"react-redux\";\r\n\r\n\r\nlet mapStateToProps = (state) => {\r\n    return {\r\n        newPostTextarea: state.profilePage.newPostTextarea,\r\n        photo: state.profilePage.profile?.photos\r\n    }\r\n}\r\n\r\n\r\nconst CreateNewPostContainer = connect(mapStateToProps, {changeTextarea, addPost}) (CreateNewPost);\r\n\r\n\r\n\r\n\r\n\r\n\r\nexport default CreateNewPostContainer;\r\n","D:\\Programming\\React\\projects\\01_firts_project\\portfolio_project\\src\\components\\MainPageComponents\\Messages\\MessagesContainer.js",["220"],"import React from 'react';\r\nimport {\r\n    SendMessage\r\n} from \"